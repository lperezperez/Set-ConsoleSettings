{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"title": "Luigitech (base16)",
	"final_space": true,
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "session",
					"style": "diamond",
					"background": "lightWhite",
					"foreground": "black",
					"trailing_diamond": "\uE0B0",
					"properties": {
						"display_user": false,
						"prefix": " \uFCBE "
					}
				},
				{
					"type": "os",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"background": "magenta",
					"foreground": "black",
					"properties": {
						"wsl": "\uE70F",
						"wsl_separator": " \uE0B1 ",
						"prefix": " "
					}
				},
				{
					"type": "shell",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"background": "red",
					"foreground": "black",
					"properties": {
						"mapped_shell_names": {
							"azure": "\uFD03",
							"bash": "\uFCB5",
							"powershell": "\uE795",
							"pwsh": "\uE795",
							"zsh": "\uE7A2"
						}
					}
				},
				{
					"type": "session",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"background": "cyan",
					"foreground": "black",
					"properties": {
						"template": "{{if .Root}}<white>\uF234 {{.UserName}}</>{{else}}\uF007 {{.UserName}}{{end}}"
					}
				},
				{
					"type": "path",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"background": "green",
					"foreground": "black",
					"properties": {
						"prefix": " \uE5FF ",
						"style": "folder"
					}
				},
				{
					"type": "git",
					"style": "powerline",
					"powerline_symbol": "\uE0B0",
					"background": "lightGreen",
					"foreground": "black",
					"properties": {
						"branch_max_length": 7,
						"display_status": true,
						"display_stash_count": true,
						"display_worktree_count": true,
						"display_upstream_icon": true,
						"status_colors_enabled": true,
						"local_changes_color": "lightYellow",
						"ahead_and_behind_color": "lightYellow",
						"prefix": " "
					}
				},
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "\uE0C0",
					"display_exit_code": false,
					"foreground": "black",
					"background": "lightRed"
				}
			]
		}
	],
	"tooltips": [
		{
			"type": "git",
			"tips": ["git", "g"],
			"style": "powerline",
			"foreground": "black",
			"background": "lightBlue",
			"properties": {
				"branch_max_length": 7,
				"display_status": true,
				"display_stash_count": true,
				"display_worktree_count": true,
				"display_upstream_icon": true,
				"status_colors_enabled": true,
				"local_changes_color": "lightYellow",
				"ahead_and_behind_color": "lightYellow",
				"prefix": " "
			}
		},
		{
			"type": "spotify",
			"tips": ["spotify", "sp"],
			"style": "powerline",
			"powerline_symbol": "\uE0B0",
			"foreground": "black",
			"background": "lightGreen",
			"properties": {
				"prefix": "\uF9C6 "
			}
		}
	]
}
